<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.budget.BudgetFragment">

    <ExpandableListView
        android:dividerHeight="0dp"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layoutDirection="rtl"
        android:id="@+id/expandableListView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tranzactionPieChart"
        app:layout_constraintVertical_bias="0.327"
        app:layout_constraintWidth_percent="0.97">

    </ExpandableListView>

    <TextView
        android:id="@+id/startDate"
        android:layout_width="91dp"
        android:layout_height="26dp"
        android:text="03/01/2020"
        android:typeface="monospace"
        app:layout_constraintBottom_toTopOf="@+id/tranzactionPieChart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.296" />

    <TextView
        android:id="@+id/endDate"
        android:layout_width="91dp"
        android:layout_height="26dp"
        android:text="03/31/2020"
        android:typeface="monospace"
        app:layout_constraintBottom_toTopOf="@+id/tranzactionPieChart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.296" />


</androidx.constraintlayout.widget.ConstraintLayout>